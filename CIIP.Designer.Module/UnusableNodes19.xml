                </LayoutGroup>
              </LayoutGroup>
            </LayoutGroup>
          </TabbedGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="BusinessObject_DetailView">
      <Layout Id="Layout">
        <TabbedGroup Id="Tab">
          <LayoutGroup Id="常规">
            <LayoutGroup Id="NameObject">
              <LayoutItem Id="NavigationItem" ViewItem="NavigationItem" />
