          <LayoutGroup Id="C1">
            <LayoutItem Id="ManyToMany" ViewItem="AssocicationInfo.ManyToMany" />
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="BusinessForm_DetailView">
      <Layout Id="Layout">
        <LayoutGroup Id="Main">
          <TabbedGroup Id="Info">
            <LayoutGroup Id="高级设置">
              <LayoutItem Id="ItemBusinessObject.Config" ViewItem="ItemBusinessObject.Config" />
