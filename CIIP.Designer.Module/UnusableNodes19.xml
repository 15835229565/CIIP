      <Layout Id="Layout">
        <LayoutGroup Id="R1">
          <LayoutGroup Id="C3">
            <LayoutItem Id="AN" ViewItem="AssocicationInfo.Name" />
          </LayoutGroup>
          <LayoutGroup Id="C2">
            <LayoutItem Id="RP" ViewItem="AssocicationInfo.RightProperty" />
            <LayoutItem Id="RT" ViewItem="AssocicationInfo.RightTable" />
            <LayoutItem Id="LP" ViewItem="AssocicationInfo.LeftProperty" />
