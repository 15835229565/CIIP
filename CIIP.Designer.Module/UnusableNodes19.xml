    <DetailView Id="PropertyBase_DetailView">
      <Layout Id="Layout">
        <LayoutGroup Id="Main">
          <LayoutGroup Id="PropertyBase">
            <LayoutItem Id="RelationProperty" ViewItem="RelationProperty" />
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
