              </LayoutGroup>
            </LayoutGroup>
            <LayoutGroup Id="常用设置">
              <LayoutGroup Id="子表设置">
                <LayoutGroup Id="R1">
                  <LayoutItem Id="ItemBusinessObject.Base" ViewItem="ItemBusinessObject.Base" />
                </LayoutGroup>
              </LayoutGroup>
              <LayoutGroup Id="主表设置">
                <LayoutGroup Id="R1">
                  <LayoutItem Id="Navigation" ViewItem="NavigationItem" />
