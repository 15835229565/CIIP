<?xml version="1.0" encoding="utf-8"?>
<Application Title="商信互联企业信息管理平台 ">
  <ActionDesign>
    <Actions>
      <Action Id="About Info" Caption="关于..." />
      <Action Id="AddFromFile" Caption="从文件填加..." />
