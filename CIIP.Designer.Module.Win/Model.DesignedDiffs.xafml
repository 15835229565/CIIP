              <LayoutItem Id="ItemBusinessObject.Config" ViewItem="ItemBusinessObject.Config" Index="5" ShowCaption="True" IsNewNode="True" />
            </LayoutGroup>
          </TabbedGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="BusinessObject_DetailView">
      <Items>
        <PropertyEditor Id="GenericParameterDefines" PropertyEditorType="CIIP.Module.Win.Editors.GenericPropertyEditor" />
        <PropertyEditor Id="Implements" PropertyEditorType="CIIP.Module.Win.Editors.ImplementPropertyEditor" />
      </Items>
    </DetailView>
    <ListView Id="SimpleType_ListView" EditorTypeName="DevExpress.ExpressApp.Win.Editors.GridListEditor">
      <Columns>
        <ColumnInfo Id="Category" Removed="True" />
      </Columns>
    </ListView>
  </Views>
