      <OwnMembers>
        <Member Name="FullName" PropertyEditorType="CIIP.Common.Win.Editors.ChooseFilePropertyEditor" />
      </OwnMembers>
    </Class>
    <Class Name="CIIP.ProjectManager.Project">
      <OwnMembers>
        <Member Name="ProjectPath" PropertyEditorType="CIIP.Common.Win.Editors.ChoosePathPropertyEditor" />
      </OwnMembers>
    </Class>
  </BOModel>
  <Options FormStyle="Ribbon" Skin="Office 2016 Colorful" UIType="TabbedMDI">
    <Dashboards DashboardGroupCaption="仪表盘" />
  </Options>
  <SchemaModules>
    <SchemaModule Name="SchedulerModuleBase" Version="18.1.5.0" IsNewNode="True" />
    <SchemaModule Name="SchedulerWindowsFormsModule" Version="18.1.5.0" IsNewNode="True" />
